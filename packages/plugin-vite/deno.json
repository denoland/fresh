{
  "name": "@fresh/plugin-vite",
  "version": "1.0.1",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts",
    "./client": "./src/client.ts"
  },
  "publish": {
    "include": ["./src", "README.md"]
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "types": ["vite/client"]
  },
  "tasks": {
    "demo": "vite demo",
    "debug": "deno run -A --inspect-brk npm:vite demo",
    "demo:build": "vite build demo",
    "demo:start": "cd demo && deno serve -A _fresh/server.js"
  },
  "imports": {
    "@babel/core": "npm:@babel/core@^7.28.0",
    "@babel/preset-react": "npm:@babel/preset-react@^7.27.1",
    "@deno/loader": "jsr:@deno/loader@^0.3.2",
    "@marvinh-test/import-json": "jsr:@marvinh-test/import-json@^0.0.1",
    "@mjackson/node-fetch-server": "npm:@mjackson/node-fetch-server@^0.7.0",
    "@prefresh/vite": "npm:@prefresh/vite@^2.4.8",
    "@radix-ui/themes": "npm:@radix-ui/themes@^3.2.1",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "@types/babel__core": "npm:@types/babel__core@^7.20.5",
    "@types/node": "npm:@types/node@^24.1.0",
    "feed": "npm:feed@^5.1.0",
    "fresh": "jsr:@fresh/core@^2.0.0",
    "ioredis": "npm:ioredis@^5.7.0",
    "mime-db": "npm:mime-db@^1.54.0",
    "pg": "npm:pg@^8.16.3",
    "preact": "npm:preact@^10.26.9",
    "rollup-plugin-visualizer": "npm:rollup-plugin-visualizer@^6.0.3",
    "vite": "npm:vite@^7.1.4",
    "vite-plugin-inspect": "npm:vite-plugin-inspect@^11.3.2"
  }
}
